.navbar-ltheme {
  background-color: #0e91a1	;
  border-color: #e7e7e7;
}
.navbar-ltheme .navbar-brand {
  color: #fff;
}
.navbar-ltheme .navbar-brand:hover,
.navbar-ltheme .navbar-brand:focus {
  color: #5e5e5e;
  background-color: transparent;
}
.navbar-ltheme .navbar-text {
  color: #fff;
}
.navbar-ltheme .navbar-nav > li > a {
  color: #fff;
  font-weight: bold;
}
.navbar-ltheme .navbar-nav > li > a:hover,
.navbar-ltheme .navbar-nav > li > a:focus {
  color: #fff;
  background-color: #146c76;
}
.navbar-ltheme .navbar-nav > .active > a,
.navbar-ltheme .navbar-nav > .active > a:hover,
.navbar-ltheme .navbar-nav > .active > a:focus {
  color: #555;
  background-color: #e7e7e7;
}
.navbar-ltheme .navbar-nav > .disabled > a,
