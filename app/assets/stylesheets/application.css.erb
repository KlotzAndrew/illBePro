.navbar-ltheme .navbar-nav > .disabled > a:focus {
  color: #ccc;
  background-color: transparent;
}
.navbar-ltheme .navbar-toggle {
  border-color: #ddd;
}
.navbar-ltheme .navbar-toggle:hover,
.navbar-ltheme .navbar-toggle:focus {
  background-color: #ddd;
}
.navbar-ltheme .navbar-toggle .icon-bar {
  background-color: #888;
}
.navbar-ltheme .navbar-collapse,
.navbar-ltheme .navbar-form {
  border-color: #e7e7e7;
}
.navbar-ltheme .navbar-nav > .open > a,
.navbar-ltheme .navbar-nav > .open > a:hover,
.navbar-ltheme .navbar-nav > .open > a:focus {
  color: #555;
  background-color: #e7e7e7;
}
@media (max-width: 767px) {
  .navbar-ltheme .navbar-nav .open .dropdown-menu > li > a {
    color: #777;
  }
  .navbar-ltheme .navbar-nav .open .dropdown-menu > li > a:hover,
  .navbar-ltheme .navbar-nav .open .dropdown-menu > li > a:focus {
    color: #333;
    background-color: transparent;
  }
  .navbar-ltheme .navbar-nav .open .dropdown-menu > .active > a,
  .navbar-ltheme .navbar-nav .open .dropdown-menu > .active > a:hover,
  .navbar-ltheme .navbar-nav .open .dropdown-menu > .active > a:focus {
    color: #555;
    background-color: #e7e7e7;
  }
  .navbar-ltheme .navbar-nav .open .dropdown-menu > .disabled > a,
  .navbar-ltheme .navbar-nav .open .dropdown-menu > .disabled > a:hover,
  .navbar-ltheme .navbar-nav .open .dropdown-menu > .disabled > a:focus {
    color: #ccc;
    background-color: transparent;
  }
}
.navbar-ltheme .navbar-link {
  color: #777;
}
.navbar-ltheme .navbar-link:hover {
  color: #333;
}
.navbar-ltheme .btn-link {
  color: #777;
}
.navbar-ltheme .btn-link:hover,
.navbar-ltheme .btn-link:focus {
  color: #333;
}
.navbar-ltheme .btn-link[disabled]:hover,
fieldset[disabled] .navbar-ltheme .btn-link:hover,
.navbar-ltheme .btn-link[disabled]:focus,
fieldset[disabled] .navbar-ltheme .btn-link:focus {
  color: #ccc;
}

