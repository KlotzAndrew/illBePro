<!DOCTYPE html>
<html>
<head>
  <title>Testapp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<nav class="navbar navbar-default navbar-fixed-top" role="navigation">
		<ul class="navbar-text navbar-right">
			<% if user_signed_in? %>
				<li><%= link_to current_user.full_name, edit_user_registration_path %></li>
				<li><%= link_to "Log Out", logout_path %></li>
			<% else %>
				<li><%= link_to "Register", register_path %></li>
				<li><%= link_to "Login", login_path %></li>
			<% end %>
		</ul>
		<div class="navbar-inner">
			<div class="container">
				<ul class="nav navbar-nav">
					<a class="navbar-brand" href=""> HeroMenu </a>
					<li><%= link_to "Challenges", challenges_path %></li>
					<li><%= link_to "Summoner", ignindices_path %></li>
					<li><%= link_to "Scoreboard", scores_path %></li>
				</ul>
				</div>
			</div>
		</nav>
	<div class="container">
		<% flash.each do |type, message| %>
			<div class="alert <%= flash_class type %> alert-dismissible" role="alert">
					<button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
					<%= message %>
				</div>
		<% end %>

		<%= yield %>
	</div>

</body>
</html>

<div>
	<p>Put me at the bottom of the page</p>
	<p>An Andrew Klotz Production</p>
	<p>League legal stuff</p>
</div>